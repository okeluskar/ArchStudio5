<?xml version='1.0' encoding='UTF-8'?>
<matrix-project plugin="matrix-project@1.3">
  <actions/>
  <description>Build and publish Buckminster&apos;s update site</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.2.5">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/isr-uci-edu/ArchStudio5.git</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions>
      <hudson.plugins.git.extensions.impl.RelativeTargetDirectory>
        <relativeTargetDir>${WORKSPACE}/git/ArchStudio5</relativeTargetDir>
      </hudson.plugins.git.extensions.impl.RelativeTargetDirectory>
    </extensions>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <axes>
    <hudson.matrix.TextAxis>
      <name>eclipse_version</name>
      <values>
        <string>4.3</string>
        <string>4.4</string>
        <string>4.5</string>
      </values>
    </hudson.matrix.TextAxis>
    <hudson.matrix.JDKAxis>
      <name>jdk</name>
      <values>
        <string>1.7</string>
      </values>
    </hudson.matrix.JDKAxis>
  </axes>
  <builders>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets> -Ddir=&quot;${WORKSPACE}&quot;
 -Ddelete=**/*
 -Dretain=git/**/*,director*,director/**/*,buckminster*,buckminster/**/*,.buckminster/tp/**/*
clean
 -Declipse.version=4.4
install-buckminster-eclipse</targets>
      <antName>(Default)</antName>
      <buildFile>${WORKSPACE}/git/ArchStudio5/org.archstudio.releng/build.xml</buildFile>
    </hudson.tasks.Ant>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets> -Declipse.version=${eclipse_version}
 -Dbuckminster.component=org.archstudio.p2.buckminster
 -Dgit.clone.dir=&quot;${WORKSPACE}/git&quot;
resolve-cquery
</targets>
      <antName>(Default)</antName>
      <buildFile>${WORKSPACE}/git/ArchStudio5/org.archstudio.releng/build.xml</buildFile>
    </hudson.tasks.Ant>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets> -Dbuckminster.component=org.archstudio.p2.buckminster
build-update-site</targets>
      <antName>(Default)</antName>
      <buildFile>${WORKSPACE}/git/ArchStudio5/org.archstudio.releng/build.xml</buildFile>
    </hudson.tasks.Ant>
  </builders>
  <publishers>
    <hudson.tasks.Mailer plugin="mailer@1.11">
      <recipients>sahendrickson@gmail.com</recipients>
      <dontNotifyEveryUnstableBuild>true</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
  <executionStrategy class="hudson.matrix.DefaultMatrixExecutionStrategyImpl">
    <runSequentially>false</runSequentially>
  </executionStrategy>
</matrix-project>